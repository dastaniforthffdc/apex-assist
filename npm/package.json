{
  "name": "apex-assist",
  "version": "0.7.0",
  "description": "Salesforce Apex language code analysis tools",
  "publisher": "KevinJones",
  "main": "dist/boot.js",
  "repository": "nawforce/apexlink",
  "author": "Kevin Jones <nawforce@gmail.com> (https://github.com/nawforce)",
  "license": "BSD-3-Clause",
  "bugs": "https://github.com/nawforce/apexlink/issues",
  "icon": "logo.png",
  "dependencies": {
    "xmldom": "0.1.27",
    "antlr4ts": "0.5.0-alpha.3",
    "apex-parser": "2.2.0"
  },
  "devDependencies": {
    "fs-monkey": "0.3.3",
    "memfs": "3.0.5"
  },
  "engines": {
    "vscode": "^1.41.0"
  },
  "activationEvents": [
    "onCommand:apex-assist.check",
    "onCommand:apex-assist.zombies",
    "onCommand:apex-assist.clear"
  ],
  "contributes": {
    "commands": [
      {
        "command": "apex-assist.check",
        "title": "ApexAssist: Check code"
      },
      {
        "command": "apex-assist.zombies",
        "title": "ApexAssist: Find zombies"
      },
      {
        "command": "apex-assist.clear",
        "title": "ApexAssist: Clear diagnostics"
      }
    ],
    "configuration": {
      "type": "Object",
      "title": "Apex Assist",
      "properties": {
        "apexAssist.extraNamespaces": {
          "type": "string",
          "default": "",
          "description": "Comma separated list of external namespaces"
        }
      }
    }
  },
  "scripts": {}
}
